{
  "name": "simple-rest-client",
  "displayName": "Simple REST Client",
  "publisher": "Tino",
  "description": "Easy to examine your APIâ€™s responses directly within Visual Studio Code. Send requests to verify your API.",
  "version": "1.2.8",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "test",
    "api",
    "postman",
    "testing",
    "http",
    "http client",
    "api testing",
    "api client",
    "request",
    "requests",
    "request client",
    "rest",
    "rest client"
  ],
  "repository": {
    "url": "https://github.com/HoangNguyen17193/vscode-simple-rest-client"
  },
  "homepage": "https://github.com/HoangNguyen17193/vscode-simple-rest-client/blob/master/README.md",
  "preview": false,
  "icon": "resources/rap-new-logo.png",
  "license": "MIT",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "RC-explorer",
          "title": "Simple REST Client",
          "icon": "resources/rap-icon.png"
        }
      ]
    },
    "views": {
      "RC-explorer": [
        {
          "id": "Menu",
          "name": ""
        }
      ]
    },
    "commands": [
      {
        "command": "RestClient.newRequest",
        "title": "Simple Rest Client: New Request"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Simple REST Client",
      "properties": {
        "history.maxStored": {
          "type": "integer",
          "default": 50,
          "description": "Max num of requests can be stored"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "clipboardy": "^2.1.0",
    "homedir": "^0.6.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
